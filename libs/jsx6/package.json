{
  "name": "@jsx6/jsx6",
  "version": "1.8.0",
  "type": "module",
  "sideEffects": false,
  "description": "JSX and ES6+ UI library",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "unpkg": "cjs/index.js",
  "module": "./index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./cjs/index.js",
      "default": "./index.js"
    }
  },
  "files": [
    "index.js",
    "src",
    "cjs"
  ],
  "scripts": {
    "test:cov": "c8 node --test",
    "build": "esbuild index.js --jsx-factory=h --jsx-fragment=null --outdir=esm --bundle --sourcemap --minify --loader:.js=jsx --format=esm",
    "build-cjs": "esbuild index.js --jsx-factory=h --jsx-fragment=null --outdir=cjs --bundle --sourcemap --minify --loader:.js=jsx --format=cjs",
    "test": "node --test",
    "prepublishOnly": "node ../../verSwitch.js p && npm run build && npm run build-cjs && npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrgdavor/jsx6.git"
  },
  "keywords": [
    "UI",
    "jsx"
  ],
  "author": "Davor hrg",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/hrgdavor/jsx6/blob/master/LICENSE.md"
    }
  ],
  "bugs": {
    "url": "https://hrgdavor/jsx6/issues"
  },
  "homepage": "https://github.com/hrgdavor/jsx6/blob/master/README.md",
  "devDependencies": {
    "@jsx6/build": "^0.2.4",
    "esbuild": "^0.17.18",
    "jsdom": "~20.0.1",
    "@jsx6/jsx-dev-runtime": "workspace:*",
    "typescript": "^5.0.4",
    "@happy-dom/global-registrator": "~13.2.0",
    "@types/bun": "~1.0.2"
  },
  "dependencies": {
    "@jsx6/signal": "workspace:*"
  }
}
