{
  "name": "@jsx6/repl",
  "version": "1.7.4",
  "type": "module",
  "description": "JSX6 REPL engine",
  "main": "index.js",
  "unpkg": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "default": "./esm/index.js"
    }
  },
  "files": [
    "index.js",
    "src",
    "esm",
    "cjs"
  ],
  "scripts": {
    "start": "node src_build/build.js --dev",
    "build": "node src_build/build.js",
    "format": "prettier --write \"./src/**/*.{js,jsx,json}\"",
    "prepublishOnly": "node ../../verSwitch.js p && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrgdavor/jsx6.git"
  },
  "keywords": [
    "UI",
    "jsx",
    "repl"
  ],
  "author": "Davor hrg",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/hrgdavor/jsx6/blob/master/LICENSE.md"
    }
  ],
  "bugs": {
    "url": "https://hrgdavor/jsx6/issues"
  },
  "homepage": "https://hrgdavor.github.io/jsx6/",
  "devDependencies": {
    "esbuild": "^0.17.18",
    "@jsx6/build": "^0.2.4",
    "@jsx6/jsx-dev-runtime": "1.7.4",
    "@trivago/prettier-plugin-sort-imports": "~3.3.0",
    "prettier": "~2.7.1",
    "@jsx6/editor-monaco": "workspace:^0.34.100",
    "live-server": "^1.2.2",
    "@babel/standalone": "~7.18.12"
  },
  "dependencies": {
    "@jsx6/jsx6": "1.7.4",
    "@jsx6/jsx-runtime": "1.7.4",
    "@jsx6/dom-observer": "2.0.0",
    "@jsx6/signal": "1.7.4",
    "babel-plugin-syntax-jsx": "~6.18.0",
    "babel-plugin-jsx-simple": "~1.6.0",
    "markdown-it": "~13.0.1",
    "mulmd": "~0.7.0",
    "perfect-scrollbar": "~1.5.5"
  }
}
