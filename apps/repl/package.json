{
  "name": "@jsx6/repl",
  "version": "1.4.3",
  "type": "module",
  "description": "JSX6 REPL engine",
  "main": "index.js",
  "unpkg": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "default": "./esm/index.js"
    }
  },
  "files": [
    "index.js",
    "src",
    "esm",
    "cjs"
  ],
  "scripts": {
    "build": "esbuild index.jsx --jsx-factory=h --jsx-fragment=null --outdir=esm --bundle --sourcemap --minify --loader:.js=jsx --format=esm --log-level=error",
    "build-cjs": "esbuild index.jsx --jsx-factory=h --jsx-fragment=null --outdir=cjs --bundle --sourcemap --minify --loader:.js=jsx --format=cjs --log-level=error",
    "format": "prettier --write \"./src/**/*.{js,jsx,json}\"",
    "prepublishOnly": "npm run build && npm run build-cjs",
    "build-local-jsx6": "esbuild ../../libs/jsx6/index.js --bundle --outfile=dist/jsx6.js --sourcemap --format=cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrgdavor/jsx6.git"
  },
  "keywords": [
    "UI",
    "jsx",
    "repl"
  ],
  "author": "Davor hrg",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/hrgdavor/jsx6/blob/master/LICENSE.md"
    }
  ],
  "bugs": {
    "url": "https://hrgdavor/jsx6/issues"
  },
  "homepage": "https://github.com/hrgdavor/jsx6/blob/master/apps/repl/README.md",
  "devDependencies": {
    "esbuild": "^0.14.39",
    "vite": "~2.9.13",
    "vite-plugin-monaco-editor": "~1.0.11",
    "@trivago/prettier-plugin-sort-imports": "~3.3.0",
    "prettier": "~2.7.1"
  },
  "dependencies": {
    "@jsx6/jsx6": "workspace:^1.4.3",
    "babel-plugin-syntax-jsx": "~6.18.0",
    "babel-plugin-jsx-simple": "~1.4.0",
    "monaco-editor": "~0.33.0",
    "@babel/standalone": "~7.18.12",
    "markdown-it": "~13.0.1",
    "mulmd": "~0.7.0",
    "perfect-scrollbar": "~1.5.5"
  }
}
