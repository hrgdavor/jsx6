{
  "name": "@jsx6/jsx6",
  "version": "1.0.0",
  "type": "module",
  "description": "JSX and ES6+ UI library",
  "main": "index.js",
  "unpkg": "dist/jsx6.min.js",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "watch": "esbuild index.js --jsx-factory=h --jsx-fragment=null --outdir=dist --bundle --sourcemap --minify --loader:.js=jsx --watch",
    "build": "esbuild index.js --jsx-factory=h --jsx-fragment=null --outdir=dist --bundle --sourcemap --minify --loader:.js=jsx",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrgdavor/jsx6.git"
  },
  "keywords": [
    "UI",
    "jsx"
  ],
  "author": "Davor hrg",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/hrgdavor/jsx6/blob/master/LICENSE.md"
    }
  ],
  "bugs": {
    "url": "https://hrgdavor/jsx6/issues"
  },
  "homepage": "https://github.com/hrgdavor/jsx6/blob/master/README.md",
  "devDependencies": {
    "esbuild": "^0.14.39"
  }
}
